  "engines": {
    "node": "14.x - 16.x"
  },
  "homepage": "https://usds.github.io/questionable",
  "keywords": [
    "component library",
    "design system",
    "questionnaire",
    "react component",
    "react",
    "survey",
    "us web design",
    "usds",
    "uswds 2.0",
    "uswds"
  ],
  "license": "ISC",
  "name": "@usds.gov/questionable-project",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "label": "TS distribution",
              "path": "dist/**"
            }
          ]
        }
      ]
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md",
          "README.md"
        ],
        "message": "release: ${nextRelease.version} [skip ci] nn${nextRelease.notes}",
        "path": "@semantic-release/git"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usds/questionable.git"
  },
  "types": "types.d.ts",
  "version": "1.0.0",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "scripts": {
    "build": "yarn workspaces foreach run build",
    "lint": "yarn workspaces foreach run lint:fix",
    "contributors:add": "all-contributors add",
    "start": "yarn workspace @usds.gov/questionable-storybook start",
    "prepare": "husky install",
    "nuke": "yarn workspaces foreach run clean && rimraf node_modules/.cache && rimraf .yarn/cache && yarn cache clean"
  },
  "workspaces": [
    "packages/*"
  ]
}
