{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "contributors": [
    "Christopher Froehlich <christopher.r.froehlich@omb.eop.gov>",
    "Tom Neilsen <thomas.a.nielsen@omb.eop.gov>"
  ],
  "dependencies": {
    "@trussworks/react-uswds": "^1.17.0",
    "class-validator": "^0.13.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "4.3.2",
    "use-wizard": "^4.0.4"
  },
  "description": "Create user friendly workflows for complex requirements, like eligibility or screener questionnaires",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@mdx-js/react": "^1.6.22",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@storybook/storybook-deployer": "^2.8.8",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/luxon": "^1.26.5",
    "@types/node": "^15.12.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "all-contributors-cli": "^6.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-align-import": "^1.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json-files": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "eslint-plugin-yaml": "^0.4.2",
    "gh-pages": "^3.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "prettier-plugin-organize-imports": "^2.1.0",
    "pretty-quick": "^3.1.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.3",
    "storybook-readme": "^5.0.9",
    "typescript-bundle": "^1.0.17"
  },
  "engines": {
    "node": "14.x - 16.x"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://usds.github.io/questionable",
  "keywords": [
    "react",
    "uswds",
    "uswds 2.0",
    "react component",
    "us web design",
    "design system",
    "component library",
    "usds",
    "survey",
    "questionnaire"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "name": "@usds.gov/questionable",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "label": "TS distribution",
              "path": "dist/**"
            }
          ]
        }
      ]
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "assets": [
          "dist/**",
          "package.json",
          "yarn.lock",
          "CHANGELOG.md",
          "README.md"
        ],
        "message": "release: ${nextRelease.version} [skip ci] nn${nextRelease.notes}",
        "path": "@semantic-release/git"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usds/questionable.git"
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "webpack": "4.44.2"
  },
  "scripts": {
    "build": "yarn type-check && tsc-bundle tsconfig.build.json",
    "build:storybook": "build-storybook -s public",
    "build:watch": "yarn build -w",
    "clean": "rimraf ./dist",
    "clean:storybook": "rimraf storybook-static",
    "contributors:add": "all-contributors add",
    "deploy": "gh-pages -d build",
    "deploy:storybook": "yarn clean:storybook && yarn build:storybook && yarn storybook-to-ghpages -- --existing-output-dir=storybook-static",
    "eject": "react-scripts eject",
    "lint": "pretty-quick --staged && yarn lint:ts",
    "lint:ts": "eslint --cache --fix \"src/**/*.{ts,tsx}\"",
    "postbuild": "yarn run copy-files",
    "prebuild": "yarn clean",
    "predeploy": "npm run build",
    "prepare": "husky install",
    "pretty": "prettier --write \"src/**/*.{js,ts,tsx,css,json}\"",
    "semantic-release": "semantic-release",
    "start": "start-storybook -p 9009 -s public",
    "test": "react-scripts test",
    "type-check": "tsc --noEmit"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.1"
}
