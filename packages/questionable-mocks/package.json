{
  "name": "@usds.gov/questionable-mocks",
  "packageManager": "yarn@3.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn lint && yarn build:tsc",
    "bundle": "yarn clean && yarn build && yarn build:rollup",
    "build:rollup": "rollup --config --environment NODE_ENV:production",
    "build:watch": "yarn build -w",
    "build:tsc": "tsc -p tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "yarn lint:ts",
    "lint:fix": "yarn pretty && pretty-quick --staged && yarn lint:ts --fix",
    "lint:ts": "eslint src/**/*.ts",
    "pretty": "prettier --write src/**/*.ts",
    "type-check": "yarn build:tsc --noEmit"
  },
  "version": "0.0.2",
  "devDependencies": {
    "@babel/core": "npm:7.12.9",
    "@babel/plugin-syntax-dynamic-import": "npm:^7.8.3",
    "@babel/plugin-transform-runtime": "npm:^7.16.4",
    "@babel/preset-env": "npm:^7.11.0",
    "@babel/runtime": "npm:^7.0.0",
    "@rollup/plugin-commonjs": "npm:^21.0.3",
    "@rollup/plugin-node-resolve": "npm:^11.2.1",
    "@swc/core": "npm:^1.2.161",
    "@swc/helpers": "npm:^0.3.8",
    "@types/babel__core": "^7",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8",
    "@types/node": "npm:*",
    "@types/prettier": "^2",
    "@types/rimraf": "^2",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@typescript-eslint/eslint-plugin": "npm:^5.17.0",
    "@typescript-eslint/parser": "npm:^5.17.0",
    "eslint": "npm:^8.12.0",
    "eslint-config-airbnb-base": "npm:^15.0.0",
    "eslint-config-prettier": "npm:^8.5.0",
    "eslint-plugin-align-assignments": "npm:^1.1.2",
    "eslint-plugin-align-import": "npm:^1.0.0",
    "eslint-plugin-flowtype": "npm:^8.0.3",
    "eslint-plugin-import": "npm:^2.25.3",
    "eslint-plugin-prettier": "npm:^4.0.0",
    "eslint-plugin-sort-keys-fix": "npm:^1.1.2",
    "eslint-plugin-typescript-sort-keys": "npm:^2.1.0",
    "prettier": "npm:>=2.2.1 <=2.3.0",
    "prettier-plugin-organize-imports": "npm:^2.3.4",
    "rimraf": "npm:^2.2.8",
    "rollup": "npm:^2.43.1",
    "rollup-plugin-copy": "npm:^3.4.0",
    "rollup-plugin-dts": "npm:^4.2.0",
    "rollup-plugin-peer-deps-external": "npm:^2.2.4",
    "rollup-plugin-terser": "npm:^7.0.0",
    "rollup-plugin-ts": "npm:^2.0.5",
    "ts-node": "npm:^10.7.0"
  },
  "dependencies": {
    "typescript": "npm:^4.6.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
