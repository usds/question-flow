  "description": "Core logic and state machine. Compatible with node, the browser and the console.",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://usds.github.io/questionable",
  "keywords": [
    "state machine",
    "logic gate",
    "linear flow"
  ],
  "main": "dist/index.js",
  "name": "@usds.gov/questionable-core",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "yarn lint && yarn build:tsc",
    "bundle": "yarn clean && yarn build && yarn build:rollup && yarn build:schema",
    "build:rollup": "rollup --config --environment NODE_ENV:production",
    "build:schemaJson": "ts-json-schema-generator -p './src/survey/*.ts' -o './dist/survey.json' --no-type-check --additional-properties true",
    "build:schema": "yarn build:schemaJson && ts-node ./parseSchema.ts",
    "build:watch": "yarn build -w",
    "build:tsc": "tsc -p tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "yarn lint:ts",
    "lint:fix": "yarn pretty && pretty-quick --staged && yarn lint:ts --fix",
    "lint:ts": "eslint src/**/*.ts",
    "pretty": "prettier src/**/*.ts",
    "type-check": "yarn build:tsc --noEmit"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.2"
}
