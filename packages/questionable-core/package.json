    "@types/luxon": "npm:^2.3.1",
    "@types/node": "npm:*",
    "@types/uuid": "npm:^8.3.4",
    "@typescript-eslint/eslint-plugin": "npm:^5.17.0",
    "@typescript-eslint/parser": "npm:^5.17.0",
    "@usds.gov/questionable-build": "workspace:*",
    "babel-loader": "npm:8.2.4",
    "cross-env": "^7.0.3",
    "eslint": "npm:^8.12.0",
    "eslint-config-airbnb-base": "npm:^15.0.0",
    "eslint-config-prettier": "npm:^8.5.0",
    "eslint-plugin-align-assignments": "npm:^1.1.2",
    "eslint-plugin-align-import": "npm:^1.0.0",
