{
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "serialize-error": "9.1.0"
  },
  "engines": {
    "node": "14.x - 16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.180",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-loader": "^8.2.4",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-align-import": "^1.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.1.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "semantic-release": "^19.0.2",
    "ts-json-schema-generator": "^1.0.0",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "description": "Core logic and state machine. Compatible with node, the browser and the console.",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://usds.github.io/questionable",
  "keywords": [
    "state machine",
    "logic gate",
    "linear flow"
  ],
  "main": "dist/index.js",
  "name": "@usds.gov/questionable-core",
  "scripts": {
    "build": "yarn lint && yarn build:tsc && yarn build:rollup && yarn build:schema",
    "build:rollup": "rollup --config --environment NODE_ENV:production",
    "build:schema": "ts-json-schema-generator -p './src/survey/*.ts' -o './dist/survey.json' --no-type-check --additional-properties true && node ./parseSchema.js",
    "build:watch": "yarn build -w",
    "build:tsc": "tsc -p tsconfig.json",
    "clean": "rimraf ./dist",
    "deploy": "npm publish",
    "lint": "yarn lint:ts && yarn type-check",
    "lint:fix": "pretty-quick --staged && eslint --fix \"src/**/*.{ts,tsx,json,js}\"",
    "lint:ts": "eslint \"src/**/*.{ts,tsx,json,js}\"",
    "prebuild": "yarn clean",
    "pretty": "prettier --write \"src/**/*.{js,ts,tsx,css,json}\"",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.2"
}
