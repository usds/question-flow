{
  "browser": "dist/index.umd.js",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@ramonak/react-progress-bar": "^4.4.0",
    "@rjsf/core": "^3.2.1",
    "@rjsf/semantic-ui": "^3.2.1",
    "@trussworks/react-uswds": "^2.7.2",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-ui-react": "^2.1.1",
    "serialize-error": "^9.1.0",
    "use-wizard": "^4.0.6"
  },
  "description": "Create user friendly workflows for complex requirements, like eligibility or screener questionnaires",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://usds.github.io/questionable",
  "keywords": [
    "component library",
    "design system",
    "questionnaire",
    "react component",
    "react",
    "survey",
    "us web design",
    "usds",
    "uswds 2.0",
    "uswds"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "name": "@usds.gov/questionable",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "label": "TS distribution",
              "path": "dist/**"
            }
          ]
        }
      ]
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md",
          "README.md"
        ],
        "message": "release: ${nextRelease.version} [skip ci] nn${nextRelease.notes}",
        "path": "@semantic-release/git"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usds/questionable.git"
  },
  "resolutions": {
    "@types/react": "^17.0.29"
  },
  "scripts": {
    "build": "yarn lint && yarn build:rollup && yarn build:schema",
    "build:rollup": "rollup --config --environment NODE_ENV:production",
    "build:schema": "ts-json-schema-generator -p './src/survey/*.ts' -o './dist/survey.json' --no-type-check --additional-properties true && node ./scripts/parseSchema.js",
    "build:storybook": "build-storybook -s public",
    "build:watch": "yarn build -w",
    "clean": "rimraf ./dist",
    "clean:storybook": "rimraf storybook-static",
    "contributors:add": "all-contributors add",
    "deploy": "gh-pages -d build",
    "deploy:storybook": "yarn clean:storybook && yarn build:storybook && yarn deploy:_storybook",
    "deploy:_storybook": "yarn storybook-to-ghpages -- -e=storybook-static --source-branch=main --host-token-env-variable=GITHUB_TOKEN --ci",
    "lint": "yarn lint:ts && yarn type-check",
    "lint:fix": "pretty-quick --staged && eslint --cache --fix \"src/**/*.{ts,tsx,json,js}\"",
    "lint:ts": "eslint \"src/**/*.{ts,tsx,json,js}\"",
    "nuke": "yarn clean && yarn clean:storybook && rimraf node_modules/.cache && rimraf .yarn/cache && yarn cache clean",
    "postbuild": "yarn run copy-files",
    "prebuild": "yarn clean",
    "predeploy": "npm run build",
    "prepare": "husky install",
    "pretty": "prettier --write \"src/**/*.{js,ts,tsx,css,json}\"",
    "semantic-release": "semantic-release",
    "start": "start-storybook -p 9009 -s public",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "version": "1.5.4",
  "packageManager": "yarn@3.1.1"
}
