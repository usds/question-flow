{
  "name": "@usds.gov/questionable-build",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "yarn lint && yarn build:tsc",
    "bundle": "yarn clean && yarn build && yarn build:rollup",
    "build:rollup": "rollup --config --environment NODE_ENV:production",
    "build:tsc": "tsc -p tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "yarn lint:ts",
    "type-check": "yarn build:tsc --noEmit",
    "lint:fix": "pretty-quick --staged && yarn lint:ts --cache --fix",
    "lint:ts": "eslint \"src/**/*.{ts,tsx,json,js}\"",
    "pretty": "prettier --write \"src/**/*.{js,ts,tsx,css,json}\""
  },
  "version": "0.0.1",
  "dependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@swc/core": "^1.2.168",
    "@swc/helpers": "^0.3.8",
    "@types/babel__core": "^7",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8",
    "@types/node": "*",
    "@types/prettier": "^2",
    "@types/rimraf": "^3",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-align-import": "^1.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "devDependencies": {
    "@types/eslint": "^8",
    "eslint": "^8.13.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
